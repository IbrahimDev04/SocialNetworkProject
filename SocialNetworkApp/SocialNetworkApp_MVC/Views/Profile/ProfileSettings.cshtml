                                        <div class="flex-1 max-md:mt-4">
                                            <textarea asp-for="UserProfileVM.About" class="w-full" rows="5" placeholder="@Model.UserProfileVM.About"></textarea>
                                        </div>
                                    </div>

                                    <div class="md:flex items-center gap-10">
